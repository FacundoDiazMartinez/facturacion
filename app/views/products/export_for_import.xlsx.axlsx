wb = xlsx_package.workbook
title = wb.styles.add_style(:bg_color => "80CBC4", :fg_color=>"#FF000000", :sz=>14,  :border=> {:style => :thin, :color => "80CBC4"})
wb.add_worksheet(name: "Productos") do |sheet|
    sheet.add_row(["Categoría", "(*)Código Interno", "(*)Nombre", "Código de Proveedor", "(*)Precio de costo", "(*)(%)I.V.A.", "($)Precio Bruto", "(*)($)Precio Final", "Medida", "Unidad de medida", "Stock"], :style=> title)
end
