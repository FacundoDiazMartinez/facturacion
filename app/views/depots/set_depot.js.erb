<% if not @depot.errors.any? %>
	$("#depotModal").modal('hide');
	$('#depotModal').on('hidden.bs.modal', function (e) {
		<% if session[:return_to].blank? %>
  		$("#depots_table").html("<%= j render '/depots/depots_table' %>");
		<% else %>
			<% flash[:notice] = "Ahora puede continuar con la operaciÃ³n." %>
			var new_path = '<%= session.delete(:return_to) %>';
			document.location.href = new_path;
		<% end %>
	});
<% else %>
	$("#depot_errors").html("<%= j error_explanation(@depot) %>");
<% end %>
